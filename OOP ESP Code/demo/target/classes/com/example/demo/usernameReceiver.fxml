   <children>
      <Label layoutX="10.0" layoutY="10.0" text="Enter Username" />
      <TextField id="username" fx:id="username" layoutX="110.0" layoutY="10.0" />
      <Button id="confirmUsername" fx:id="confirmUsername" layoutX="115.0" layoutY="50.0" maxWidth="65.0" mnemonicParsing="false" onAction="#Confirm" prefHeight="20.0" text="Confirm" />
   </children>
</AnchorPane>
